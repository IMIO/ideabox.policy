        <h1 tal:content="context/Title"></h1>

        <div id="timeline" class="steps_5">
            <ol id="timeline-line"><tal:block tal:repeat="date time_line"><tal:block tal:define="step_index repeat/date/index"><li tal:attributes="class python: step_index == last_step and 'current' or ''">
                    <div class="step">
                      <tal:block tal:repeat="part_date date">
                        <div tal:content="part_date" />
                      </tal:block>
                    </div></li></tal:block></tal:block> </ol>

            <ol id="timeline-text"><tal:block tal:repeat="title time_line_title"><li tal:attributes="class python: current_state == title and 'current' or ''"><div tal:content="title"/></li></tal:block></ol>
        </div>

        <div tal:content="structure here/@@rate-if-you-dare"></div>


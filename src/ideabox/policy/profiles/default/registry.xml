<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <records interface="plone.app.discussion.interfaces.IDiscussionSettings">
    <value key="globally_enabled">True</value>
    <value key="edit_comment_enabled">True</value>
    <value key="moderation_enabled">True</value>
    <value key="show_commenter_image">False</value>
    <value key="anonymous_comments">False</value>
	</records>
  <record name="cpskin.core.social.allowed_states"
          i18n:domain="cpskin">
      <field type="plone.registry.field.List">
          <title i18n:translate="">Allowed states for social viewlet</title>
          <key_type type="plone.registry.field.TextLine" />
          <value_type type="plone.registry.field.TextLine" />
      </field>
      <value>
        <element>published_and_shown</element>
        <element>published_and_hidden</element>
        <element>deposited</element>
        <element>project_analysis</element>
        <element>vote</element>
        <element>result_analysis</element>
        <element>selected</element>
        <element>rejected</element>
        <element>study_in_progress</element>
        <element>in_progress</element>
        <element>realized</element>
      </value>
  </record>
  <record name="ideabox.new.project.folder" i18n:domain="ideabox.policy">
    <field type="plone.registry.field.TextLine">
      <title i18n:translate="">new project submission folder</title>
      <required>True</required>
    </field>
    <value>projets</value>
  </record>

  <record name="ideabox.vocabulary.district"
          i18n:domain="ideabox.policy">
      <field type="plone.registry.field.Dict">
          <title i18n:translate="">District vocabulary values</title>
          <key_type type="plone.registry.field.TextLine" />
          <value_type type="plone.registry.field.TextLine" />
      </field>
      <value>
        <element key="cle1">valeur1</element>
        <element key="cle2">valeur2</element>
        <element key="cle3">valeur3</element>
        <element key="cle4">valeur4</element>
      </value>
  </record>

  <record name="ideabox.vocabulary.theme"
          i18n:domain="ideabox.policy">
      <field type="plone.registry.field.Dict">
          <title i18n:translate="">Theme vocabulary values</title>
          <key_type type="plone.registry.field.TextLine" />
          <value_type type="plone.registry.field.TextLine" />
      </field>
      <value>
        <element key="ACPT">Art, Culture, Patrimoine, Tourisme</element>
        <element key="EVCA">Des espaces verts, des espaces collectifs, des espaces apaisés</element>
        <element key="IDNA">Idées non attribuées</element>
        <element key="INCL">Inclassables</element>
        <element key="INSO">Inclusion sociale</element>
        <element key="MOBI">La mobilité</element>
        <element key="TREN">La transition énergétique</element>
        <element key="VEAU">La végétalisation et l'agriculture urbaine</element>
        <element key="VPCN">Ville participative, Collaborative et Numérique</element>
      </value>
  </record>

</registry>
